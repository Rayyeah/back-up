        initComponents();
        setImage();
        initializeSpinners();
        setTime();
        reset();
    }
    
        private void initializeSpinners() {
    // Setting an initial value and the range for each spinner to prevent negative value
    jSpinner.setModel(new SpinnerNumberModel(0, 0, 100, 1)); // Min = 0, Max = 100, Step = 1
    jSpinner1.setModel(new SpinnerNumberModel(0, 0, 100, 1));
    jSpinner2.setModel(new SpinnerNumberModel(0, 0, 100, 1));
    jSpinner3.setModel(new SpinnerNumberModel(0, 0, 100, 1));
    jSpinner4.setModel(new SpinnerNumberModel(0, 0, 100, 1));
    jSpinner5.setModel(new SpinnerNumberModel(0, 0, 100, 1));
    jSpinner6.setModel(new SpinnerNumberModel(0, 0, 100, 1));
    jSpinner7.setModel(new SpinnerNumberModel(0, 0, 100, 1));
    jSpinner8.setModel(new SpinnerNumberModel(0, 0, 100, 1));
    jSpinner10.setModel(new SpinnerNumberModel(0, 0, 100, 1));
    jSpinner11.setModel(new SpinnerNumberModel(0, 0, 100, 1));
    jSpinner12.setModel(new SpinnerNumberModel(0, 0, 100, 1));
}
    
  
    
    public void setImage() {
    JLabel[] labels = {
        jLabelimage1, jLabelimage2, jLabelimage3, jLabelimage4,
        jLabelimage5, jLabelimage6, jLabelimage7, jLabelimage8,
        jLabelimage9, jLabelimage10, jLabelimage11, jLabelimage12
    };

    for (int i = 1; i <= labels.length; i++) {
        // Construct the image path dynamically
        String imagePath = "/images/" + i + ".png";
        ImageIcon icon = new ImageIcon(getClass().getResource(imagePath));
        
        // Error handling: check if image is loaded successfully
        if (icon.getImageLoadStatus() != MediaTracker.COMPLETE) {
            System.out.println("Error: Image not found for " + imagePath);
            continue;
        }

        // Scale the image to fit the corresponding JLabel
        Image img = icon.getImage().getScaledInstance(
            labels[i - 1].getWidth(), labels[i - 1].getHeight(), Image.SCALE_SMOOTH
        );

        // Set the scaled image as the icon of the JLabel
        labels[i - 1].setIcon(new ImageIcon(img));
    }
}

    public boolean qtyIsZero(int qty){
        System.out.println("qtyIsZero() called. Quantity: " + qty);
        
    if(qty==0){
        System.out.println("Quantity is zero. Showing warning message."); 
        JOptionPane.showMessageDialog(null,"Please indicate the item quantity");
        return false;
    }
    System.out.println("Quantity is valid.");
    return true;
}

      
    public void reset() {
        total = 0.0;
        x = 0;
        double tax = 0.0;
        btnTotal.setEnabled(true);
        jSpinner.setValue(0);
        jSpinner1.setValue(0);
        jSpinner2.setValue(0);
        jSpinner3.setValue(0);
        jSpinner4.setValue(0);
        jSpinner5.setValue(0);
        jSpinner6.setValue(0);
        jSpinner7.setValue(0);
        jSpinner8.setValue(0);
        jSpinner10.setValue(0);
        jSpinner11.setValue(0);
        jSpinner12.setValue(0);
        jTextFieldSubtotal.setText("0.0");
        jTextFieldTax.setText("0.0");
        jTexTotal.setText("0.0");
        jTextArea.setText ("");
}
    
    public void setTime(){
        new Thread(new Runnable(){
            @Override
            public void run (){
                while(true){
                    try {
                        Thread.sleep(1000);
                    } catch (InterruptedException ex) {
                        Logger.getLogger(DashboardCashier.class.getName()).log(Level.SEVERE, null, ex);
                    }
                    Date date = new Date();
                    SimpleDateFormat tf = new SimpleDateFormat("h:mm:ss aa");
                    SimpleDateFormat df = new SimpleDateFormat("EEEE, dd-MM-yyyy");
                    String time = tf.format(date);
                    jTexTime.setText(time.split(" ")[0]+" "+time.split(" ")[1]);
                    jTextDate.setText(df.format(date));
                }
            }           
        }).start();
    }
      
    public void rayCafe() {
    // Set initial header and information when the cafe details are displayed
          jTextArea.setText("******************************\n"
        + "Tax: \t\t\t"+getTax+"\n"
        + "Sub Total: \t\t\t"+total+"\n"
        + "Total: \t\t\t"+(total+getTax)+"\n\n"
        + "*****************Thank You**************\n");
    
    jTextArea.setText("************ Ray's Cafe ************\n"
        + "Time: " + jTexTime.getText() + " Date: " + jTextDate.getText() + "\n"
        + "-------------------------------------\n"
        + "Item Name: \tQty: \tPrice:\n"  // Correct alignment for the header
        + "-------------------------------------\n");

}
    
    public void getTax (int t){
       if (total > 10.0 && total <= 20.0) {
           double tax = 0.05; // 5% tax
    } else if (total > 20.0 && total <= 40.0) {
           double tax = 0.10; // 10% tax
    } else if (total > 40.0 && total <= 60.0) {
           double tax = 0.12; // 12% tax
    } else if (total > 60.0 && total <= 80.0) {
           double tax = 0.10; // 10% tax
    } else if (total > 80.0 && total <= 100.0) {
           double tax = 0.08; // 8% tax
    } else if (total > 100.0 && total <= 150.0) {
           double tax = 0.06; // 6% tax
    } else if (total > 150.0 && total <= 200.0) {
           double tax = 0.05; // 5% tax
    } else if (total > 200.0) {
           double tax = 0.075; // 7.5% tax
    }
    }
    
private void handleCheckboxAction(String itemName, double itemPrice, JSpinner qtySpinner, JCheckBox checkBox) {
    int qty = (int) qtySpinner.getValue(); // Get the value from JSpinner

    if (qty <= 0) { // Prevent negative or zero quantities
        checkBox.setSelected(false); 
        JOptionPane.showMessageDialog(this, "Please indicate the item quantity.", "Warning", JOptionPane.WARNING_MESSAGE);
        return;
    }
    x++;

    if (x == 1) {
        rayCafe(); // Add the header only once
    }
    double totalPrice = qty * itemPrice;
    total += totalPrice;
    getTax((int) total);

    jTextArea.append(x + ". " + itemName + "\t" + qty + "\t₱" + String.format("%.2f", totalPrice) + "\n");
}

    
          
